{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/settings",
        "dest": "api/index.js"
      },
      {
        "src": "/script.js",
        "dest": "public/script.js"
      },
      {
        "src": "/style.css",
        "dest": "public/style.css"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ]
  }
  